local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/stysscythe/script/main/LibTest.lua"))()
local Window = Library.Window('KR4K Library')
local Test1 = Window.CreateTab('Key system')

Test1.CreateLabel("Join Our Discord to get the key! the key will change every once in a while")
Test1.CreateButton("Copy Discord Link", function()
setclipboard("https://discord.gg/YWJEb2WgyM")
end)
Test1.CreateTextbox("Enter Key", function(txt)
if txt == "KEY" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/Bhusant/Tdu-refined/main/Sigma"))()
end
end)
